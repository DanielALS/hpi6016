
record(ai, "$(P)$(SIG)") {
    field(DESC, "$(DESC=Address $(ADDR))")
    field(DTYP, "ARM EEPROM")
    field(INP , "@$(PORT) EEPROM $(ADDR) $(LEN=1)")
    field(SCAN, "I/O Intr")
    field(ASLO, "$(ESLO=1)")
    field(AOFF, "$(AOFF=0)")
    field(PREC, "$(PREC=2)")
    field(ADEL, "$(ADEL=1e-$(PREC))")
    field(HOPR, "$(HOPR=)")
    field(LOPR, "$(LOPR=)")
    field(EGU , "$(EGU=)")
}
