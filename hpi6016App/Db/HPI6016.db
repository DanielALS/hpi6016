
record(ai, "$(P)DoseRate-I") {
    field(DTYP, "ARM Read Param")
    field(INP , "@$(PORT) DoseRate")
    field(SCAN, "I/O Intr")
    field(LINR, "LINEAR")
    field(ESLO, "1e-2")
    field(PREC, "2")
    field(ADEL, "0.02")
    field(HOPR, "500")
    field(LOPR, "-0.5")
    field(EGU , "mR/h")
    field(FLNK, "$(P)Update:1-FOut_")
}

# Trigger other values read from driver
record(fanout, "$(P)Update:1-FOut_") {
    field(LNK1, "$(P)Lvl:1-I")
    field(LNK2, "$(P)Lvl:2-I")
    field(LNK3, "$(P)Lvl:3-I")
    field(LNK4, "$(P)Cnt:Fail-I")
    field(LNK5, "$(P)Connect-Sts")
    field(LNK6, "$(P)DataTime-I_")
    field(FLNK, "$(P)Update:2-FOut_")
    field(TSEL, "$(P)DoseRate-I.TIME")
}

record(fanout, "$(P)Update:2-FOut_") {
    field(LNK1, "$(P)Alrm:1-Sts")
    field(LNK2, "$(P)Alrm:2-Sts")
    field(LNK3, "$(P)Alrm:3-Sts")
    field(LNK4, "$(P)Alrm:Fail-Sts")
    field(LNK5, "$(P)Alrm:Oflow-Sts")
    field(LNK6, "$(P)Alrm:Pulse-Sts")
    field(FLNK, "$(P)Update:3-FOut_")
    field(TSEL, "$(P)DoseRate-I.TIME")
}

# breaks up bits found in $(P)Stat-Sts
record(fanout, "$(P)Update:3-FOut_") {
    field(FLNK, "$(P)Update:4-FOut_")
    field(TSEL, "$(P)DoseRate-I.TIME")
}

# Other calculations
record(fanout, "$(P)Update:4-FOut_") {
    field(LNK1, "$(P)Alrm:Sum-Sts_")
    field(LNK2, "$(P)DoseRate:S-I")
    field(LNK3, "$(P)Dose:1m-Calc_")
    field(LNK4, "$(P)Dose:5m-Calc_")
    field(LNK5, "$(P)Dose:1h-Calc_")
    field(LNK6, "$(P)Dose:1d-Calc_")
    field(TSEL, "$(P)DoseRate-I.TIME")
}

record(ai, "$(P)Lvl:1-I") {
    field(DTYP, "ARM Read Param")
