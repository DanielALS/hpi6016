file "eepromlongin.db"
{
  {P="\$(SYS){\$(DEV)-EE}", PORT="\$(PORT)", SIG="Pause:Hi-I", ADDR="1", DESC="Hi Pause"},
  {P="\$(SYS){\$(DEV)-EE}", PORT="\$(PORT)", SIG="Pause:Lo-I", ADDR="2", DESC="Lo Pause"},
